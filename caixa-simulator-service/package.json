{
  "name": "caixa-simulator-service",
  "version": "1.3.18",
  "type": "module",
  "description": "API Node.js que realiza simulações de financiamento diretamente no site da Caixa, usando Puppeteer.",
  "main": "src/index.js",
  "scripts": {
    "dev-worker": "cross-env NODE_ENV=development nodemon src/queues/caixaSimulatorQueue.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "test": "cross-env NODE_ENV=test vitest run --coverage",
    "staging": "cross-env NODE_ENV=staging nodemon src/index.js",
    "start": "node src/index.js",
    "production": "cross-env NODE_ENV=production nodemon src/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "bullmq": "^5.53.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "googleapis": "^152.0.0",
    "https": "^1.0.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.3",
    "puppeteer": "^22.15.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "mongodb-memory-server": "^8.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.0",
    "vitest": "^3.2.4"
  }
}
