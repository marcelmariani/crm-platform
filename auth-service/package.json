{
  "name": "auth-service",
  "version": "1.0.180",
  "description": "API de autenticação via JWT, gerenciamento de usuários e hierarquia de grupos",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "bootstrap:auth:develop": "cross-env NODE_ENV=development node src/utils/run-bootstrap-once.js",
    "bootstrap:auth:staging": "cross-env NODE_ENV=staging node src/utils/run-bootstrap-once.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "lint": "npx eslint src --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "test": "cross-env NODE_ENV=test vitest run --coverage",
    "staging": "cross-env NODE_ENV=staging nodemon src/index.js",
    "start": "node src/index.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.0",
    "serverless-http": "^3.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "mongodb-memory-server": "^8.0.0",
    "nodemon": "^3.0.3",
    "serverless": "^3.40.0",
    "serverless-offline": "^13.9.0",
    "serverless-prune-plugin": "^2.1.0",
    "supertest": "^6.3.0",
    "vitest": "^3.2.4"
  }
}
